<Application x:Class="Nighthold_Login.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="NightholdLogin.xaml" Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary  Source="Styles/Buttons.xaml"></ResourceDictionary>
                <ResourceDictionary  Source="Styles/CheckBoxes.xaml"></ResourceDictionary>
                <ResourceDictionary  Source="Styles/TextBoxes.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
