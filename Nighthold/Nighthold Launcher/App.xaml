<Application  x:Class="MagicStorm_Launcher.App"
              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              StartupUri="MagicStormLauncher.xaml" Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary  Source="Styles/Buttons.xaml"></ResourceDictionary>
                <ResourceDictionary  Source="Styles/CheckBoxes.xaml"></ResourceDictionary>
                <ResourceDictionary  Source="Styles/ComboBoxes.xaml"></ResourceDictionary>
                <ResourceDictionary  Source="Styles/DatePickers.xaml"></ResourceDictionary>
                <ResourceDictionary  Source="Styles/ProgressBars.xaml"></ResourceDictionary>
                <ResourceDictionary  Source="Styles/ScrollViewerOne.xaml"></ResourceDictionary>
                <ResourceDictionary  Source="Styles/ScrollViewerTwo.xaml"></ResourceDictionary>
                <ResourceDictionary  Source="Styles/TextBoxes.xaml"></ResourceDictionary>
                <ResourceDictionary  Source="Styles/Tooltips.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
